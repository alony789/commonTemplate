{
    "name": "vite-cli",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "dev": "pnpm -r --parallel --filter=ganwei-iotcenter-* run dev",
        "build": "pnpm --filter \"ganwei-iotcenter-*\" build",
        "updateModule": "pnpm --filter \"ganwei-iotcenter-*\" updateModule",
        "download": "node ./configuration/gitClone.js",
        "cleanAll": "node ./configuration/cleanNodeModule.js",
        "public": "npm config set registry=https://mirrors.cloud.tencent.com/npm/"
    },
    "dependencies": {},
    "pnpm": {
        "packageExtensions": {
            "vue-template-compiler": {
                "peerDependencies": {
                    "vue": "2.6.14"
                }
            }
        }
    }
}